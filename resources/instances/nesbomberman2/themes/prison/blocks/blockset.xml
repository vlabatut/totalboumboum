	<abstractBlocks >
		<block name="common" folder="_common" />
		
		<group name="softwalls" folder="softwalls">
			<block name="common_soft" folder="_common"/>
		</group>

		<group name="hardwalls" folder="hardwalls">
			<block name="common_hard" folder="_common"/>
		</group>
	</abstractBlocks>
	
	<concreteBlocks>
		<group name="softwalls" folder="softwalls">
			<block name="regular" folder="regular"/>
			<block name="broken" folder="broken"/>
		</group>
		
		<group name="hardwalls" folder="hardwalls">
			<block name="regular" folder="regular"/>
			<block name="broken" folder="broken"/>
			
			<block name="horizontalA" folder="horizontalA"/>
			<block name="horizontalAbroken" folder="horizontalAbroken"/>
			<block name="horizontalB" folder="horizontalB"/>
			<block name="horizontalBbroken" folder="horizontalBbroken"/>
			<block name="horizontalC" folder="horizontalC"/>
			<block name="horizontalCbroken" folder="horizontalCbroken"/>
			<block name="leftA" folder="leftA"/>
			<block name="leftAbroken" folder="leftAbroken"/>
			<block name="leftB" folder="leftB"/>
			<block name="leftBbroken" folder="leftBbroken"/>
			<block name="leftC" folder="leftC"/>
			<block name="leftCbroken" folder="leftCbroken"/>
			<block name="rightA" folder="rightA"/>
			<block name="rightAbroken" folder="rightAbroken"/>
			<block name="rightB" folder="rightB"/>
			<block name="rightBbroken" folder="rightBbroken"/>
			<block name="rightC" folder="rightC"/>
			<block name="rightCbroken" folder="rightCbroken"/>
			<block name="upleft" folder="upleft"/>
			<block name="upright" folder="upright"/>
			<block name="vertical" folder="vertical"/>
			<block name="verticalBroken" folder="verticalBroken"/>
		</group>
	</concreteBlocks>
	
</blockset>
