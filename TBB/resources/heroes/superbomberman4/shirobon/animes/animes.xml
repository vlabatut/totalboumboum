	<colors folder="colormaps" >
		<colormap name="BLACK" file="black" />
		<colormap name="BLUE" file="blue" />
		<colormap name="BROWN" file="brown" />
		<colormap name="CYAN" file="cyan" />
		<colormap name="GRASS" file="grass" />
		<colormap name="GREEN" file="green" />
		<colormap name="GREY" file="grey" />
		<colormap name="INDIGO" file="indigo" />
		<colormap name="ORANGE" file="orange" />
		<colormap name="PINK" file="pink" />
		<colormap name="PURPLE" file="purple" />
		<colormap name="RED" file="red" />
		<colormap name="RUST" file="rust" />
		<colormap name="ULTRAMARINE" file="ultramarine" />
		<colormap name="WHITE" file="white" />
		<colormap name="YELLOW" file="yellow" />
	</colors>
	
	<shadows folder="pictures">
		<shadow name="regular" file="regular.png"/>
		<shadow name="burn" file="burn.png"/>
	</shadows>
	
	<gestures folder="pictures" >
		<gesture name="APPEARING" folder="disappearing" shadow="regular" >
			<direction name="NONE">
				<step duration="60" file="9.gif" />
				<step duration="80" file="8.gif" />
				<step duration="90" file="7.gif" />
				<step duration="100" file="6.gif" />
				<step duration="110" file="5.gif" />
				<step duration="120" file="4.gif" />
				<step duration="130" file="3.gif" />
				<step duration="140" file="2.gif" />
				<step duration="200" file="1.gif" />
			</direction>
		</gesture>
		
		<gesture name="BOUNCING" folder="bouncing" shadow="regular" shadowYShift="-5">
			<direction name="DOWN" folder="down">
				<step file="1.gif" />
			</direction>
			<direction name="LEFT" folder="left">
				<step file="1.gif" />
			</direction>
			<direction name="RIGHT" folder="right">
				<step file="1.gif" />
			</direction>
			<direction name="UP" folder="up">
				<step file="1.gif" />
			</direction>
		</gesture>
		
		<gesture name="BURNING" folder="burning">
			<direction name="NONE">
				<step duration="180" file="1.gif" shadow="regular" />
				<step duration="150" file="2.gif" shadow="burn" />
				<step duration="120" file="3.gif" shadow="burn" />
				<step duration="125" file="4.gif" shadow="burn" />
			</direction>
		</gesture>
		
		<gesture name="CARRYING" folder="carrying" shadow="regular" repeat="true" >
			<direction name="DOWN" folder="down" >
				<step duration="240" file="lf.gif" />
				<step duration="240" file="nf.gif" />
				<step duration="240" file="rf.gif" />
				<step duration="240" file="nf.gif" />
			</direction>
			<direction name="LEFT" folder="left" shadowXShift="-1">
				<step duration="240" file="lf.gif" />
				<step duration="240" file="nf.gif" />
				<step duration="240" file="rf.gif" />
				<step duration="240" file="nf.gif" />
			</direction>
			<direction name="RIGHT" folder="right" shadowXShift="2">
				<step duration="240" file="lf.gif" />
				<step duration="240" file="nf.gif" />
				<step duration="240" file="rf.gif" />
				<step duration="240" file="nf.gif" />
			</direction>
			<direction name="UP" folder="up" >
				<step duration="240" file="lf.gif" />
				<step duration="240" file="nf.gif" />
				<step duration="240" file="rf.gif" />
				<step duration="240" file="nf.gif" />
			</direction>
		</gesture>
		
		<gesture name="CRYING" folder="crying" shadow="regular" repeat="true">
			<direction name="NONE" >
				<step duration="500" file="center.gif" />
				<step duration="100" file="left.gif" />
				<step duration="100" file="center.gif" />
				<step duration="100" file="right.gif" />
				<step duration="100" file="center.gif" />
				<step duration="100" file="left.gif" />
				<step duration="100" file="center.gif" />
				<step duration="100" file="right.gif" />
				<step duration="100" file="center.gif" />
				<step duration="100" file="left.gif" />
				<step duration="100" file="center.gif" />
				<step duration="100" file="right.gif" />
				<step duration="100" file="center.gif" />
				<step duration="100" file="left.gif" />
				<step duration="100" file="center.gif" />
				<step duration="100" file="right.gif" />
				<step duration="100" file="center.gif" />
				<step duration="100" file="left.gif" />
				<step duration="100" file="center.gif" />
				<step duration="100" file="right.gif" />
				<step duration="100" file="center.gif" />
			</direction>
		</gesture>
		
		<gesture name="DISAPPEARING" folder="disappearing" shadow="regular" >
			<direction name="NONE" >
				<step duration="200" file="1.gif" />
				<step duration="140" file="2.gif" />
				<step duration="130" file="3.gif" />
				<step duration="120" file="4.gif" />
				<step duration="110" file="5.gif" />
				<step duration="100" file="6.gif" />
				<step duration="90" file="7.gif" />
				<step duration="80" file="8.gif" />
				<step duration="60" file="9.gif" />
			</direction>
		</gesture>

		<gesture name="EXULTING" folder="exulting" repeat="true" shadow="regular" >
			<direction name="NONE">
				<step duration="200" file="1.gif" />
				<step duration="50" file="2.gif" />
			</direction>
		</gesture>
		
		<gesture name="HOLDING" folder="carrying" shadow="regular" >
			<direction name="DOWN" folder="down" >
				<step file="nf.gif" />
			</direction>
			<direction name="LEFT" folder="left" shadowXShift="-1" >
				<step file="nf.gif" />
			</direction>
			<direction name="RIGHT" folder="right" shadowXShift="2" >
				<step file="nf.gif" />
			</direction>
			<direction name="UP" folder="up" >
				<step file="nf.gif" />
			</direction>
		</gesture>
		
		<gesture name="JUMPING" folder="jumping" shadow="regular" >
			<direction name="DOWN" folder="down">
				<step file="1.gif" />
			</direction>
			<direction name="LEFT" folder="left">
				<step file="1.gif" />
			</direction>
			<direction name="RIGHT" folder="right">
				<step file="1.gif" />
			</direction>
			<direction name="UP" folder="up">
				<step file="1.gif" />
			</direction>
		</gesture>
		
		<gesture name="LANDING" folder="jumping" shadow="regular" >
			<direction name="DOWN" folder="down">
				<step file="1.gif" />
			</direction>
			<direction name="LEFT" folder="left">
				<step file="1.gif" />
			</direction>
			<direction name="RIGHT" folder="right">
				<step file="1.gif" />
			</direction>
			<direction name="UP" folder="up">
				<step file="1.gif" />
			</direction>
		</gesture>
		
		<gesture name="PICKING" shadow="regular" folder="carrying">
			<direction name="DOWN" folder="down">
				<step duration="200" name="nf.gif" />
			</direction>
			<direction name="LEFT" folder="left">
				<step duration="200" name="nf.gif" />
			</direction>
			<direction name="RIGHT" folder="right">
				<step duration="200" name="nf.gif" />
			</direction>
			<direction name="UP" folder="up">
				<step duration="200" name="nf.gif" />
			</direction>
		</gesture>
		
		<gesture name="PUNCHING" shadow="regular" folder="punching">
			<direction name="DOWN">
				<step duration="50" name="down.gif" />
				<step duration="100" name="down.gif" yShift="2" shadowYShift="2" />
			</direction>
			<direction name="LEFT">
				<step duration="50" name="left.gif" />
				<step duration="100" name="left.gif" xShift="-3" shadowXShift="-3" />
			</direction>
			<direction name="RIGHT">
				<step duration="50" name="right.gif" />
				<step duration="100" name="right.gif" xShift="4" shadowXShift="4" />
			</direction>
			<direction name="UP">
				<step duration="50" name="up.gif" />
				<step duration="100" name="up.gif" yShift="-2" shadowYShift="-2" />
			</direction>
		</gesture>
		
		<gesture name="PUSHING" folder="pushing" repeat="true" shadow="regular">
			<direction name="DOWN" folder="down" >
				<step duration="240" file="lf.gif" />
				<step duration="240" file="nf.gif" />
				<step duration="240" file="rf.gif" />
				<step duration="240" file="nf.gif" />
			</direction>
			<direction name="LEFT" folder="left" >
				<step duration="240" file="lf.gif" />
				<step duration="240" file="nf.gif" />
				<step duration="240" file="rf.gif" />
				<step duration="240" file="nf.gif" />
			</direction>
			<direction name="RIGHT" folder="right" >
				<step duration="240" file="lf.gif" />
				<step duration="240" file="nf.gif" />
				<step duration="240" file="rf.gif" />
				<step duration="240" file="nf.gif" />
			</direction>
			<direction name="UP" folder="up" >
				<step duration="240" file="lf.gif" />
				<step duration="240" file="nf.gif" />
				<step duration="240" file="rf.gif" />
				<step duration="240" file="nf.gif" />
			</direction>
		</gesture>
		
		<gesture name="STAGGERING" folder="standing" repeat="true" shadow="regular">
			<direction name="NONE">
				<step duration="100" file="down/nf.gif" />
				<step duration="100" file="left/nf.gif" />
				<step duration="100" file="up/nf.gif" />
				<step duration="100" file="right/nf.gif" />
			</direction>
		</gesture>
		
		<gesture name="STANDING" folder="standing" shadow="regular" >
			<direction name="DOWN" folder="down">
				<step file="nf.gif" />
			</direction>
			<direction name="LEFT" folder="left">
				<step file="nf.gif" shadowXShift="-1"/>
			</direction>
			<direction name="RIGHT" folder="right">
				<step file="nf.gif" shadowXShift="2"/>
			</direction>
			<direction name="UP" folder="up">
				<step file="nf.gif" />
			</direction>
		</gesture>
		
		<gesture name="THROWING" folder="throwing" shadow="regular" >
			<direction name="DOWN">
				<step duration="150" name="down.gif" />
			</direction>
			<direction name="LEFT">
				<step duration="150" name="left.gif" />
			</direction>
			<direction name="RIGHT">
				<step duration="150" name="right.gif" />
			</direction>
			<direction name="UP">
				<step duration="150" name="up.gif" />
			</direction>
		</gesture>
		
		<gesture name="WALKING" folder="standing" repeat="true" shadow="regular" >
			<direction name="DOWN" folder="down" >
				<step duration="240" file="lf.gif" />
				<step duration="240" name="nf.gif" />
				<step duration="240" file="rf.gif" />
				<step duration="240" name="nf.gif" />
			</direction>
			<direction name="LEFT" folder="left" shadowXShift="-1">
				<step duration="240" file="lf.gif" />
				<step duration="240" name="nf.gif" />
				<step duration="240" file="rf.gif" />
				<step duration="240" name="nf.gif" />
			</direction>
			<direction name="RIGHT" folder="right" shadowXShift="2">
				<step duration="240" file="lf.gif" />
				<step duration="240" name="nf.gif" />
				<step duration="240" file="rf.gif" />
				<step duration="240" name="nf.gif" />
			</direction>
			<direction name="UP" folder="up" >
				<step duration="240" file="lf.gif" />
				<step duration="240" name="nf.gif" />
				<step duration="240" file="rf.gif" />
				<step duration="240" name="nf.gif" />
			</direction>
		</gesture>
		
		<gesture name="WAITING" repeat="true" shadow="regular" >
			<direction name="NONE">
				<step duration="300" file="throwing/down.gif" />
				<step duration="300" name="carrying/down/nf.gif" />
				<step duration="300" file="jumping/down/1.gif" />
				<step duration="300" name="bouncing/down/1.gif" />
				<step duration="300" file="crying/center.gif" />
				<step duration="300" file="crying/left.gif" />
				<step duration="300" file="crying/center.gif" />
				<step duration="300" file="crying/right.gif" />
			</direction>
		</gesture>
		
	</gestures>
</animes>
