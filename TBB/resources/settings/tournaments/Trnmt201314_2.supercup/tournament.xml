	<general name="Super-coupe de Galatasaray VI"/>

 	<notes>
 		<line value="6ème tournoi opposant les meilleurs agents de chaque édition du tournoi régulier." />
 		<line value="Attention : l'ordre de sélection des agents a un effet sur la structure du tournoi. Il faut d'abord sélectionner les deux premiers de cette année, puis ranger les agents par ordre de niveau croissant." />
 		<line value="Date: 28-12-2013" />
 	</notes>
 	
	<author value="Vincent Labatut" />

	<cup sortPlayers="NONE" >
		<leg number="0" randomizeParts="false" >
			<part name="Quart-de-finale" number="0" rank="3">
				<match local="true" name="leg1" />
				<tieBreak>
					<rankings>
						<scores type="BOMBINGS"/>
					</rankings>
					<match local="true" name="tieBreak" />
				</tieBreak>
				<players>
					<player part="-1" rank="1"/>
					<player part="-1" rank="2"/>
					<player part="-1" rank="3"/>
					<player part="-1" rank="4"/>
				</players>
			</part>
		</leg>
		
		<leg number="1" randomizeParts="false" >
			<part name="Demi-finale" number="0" rank="2">
				<match local="true" name="leg2" />
				<tieBreak>
					<rankings>
						<scores type="BOMBINGS"/>
					</rankings>
					<match local="true" name="tieBreak" />
				</tieBreak>
				<players>
					<player leg="-1" part="-1" rank="5"/>
					<player leg="-1" part="-1" rank="6"/>
					<player leg="0"  part="0"  rank="1"/>
					<player leg="0"  part="0"  rank="2"/>
					<player leg="0"  part="0"  rank="3"/>
				</players>
			</part>
		</leg>

		<leg number="2" randomizeParts="false" >
			<part name="Finale" number="0" rank="1">
				<match local="true" name="leg3" />
				<tieBreak>
					<rankings>
						<scores type="BOMBINGS"/>
					</rankings>
					<match local="true" name="tieBreak" />
				</tieBreak>
				<players>
					<player leg="-1" part="-1" rank="7"/>
					<player leg="-1" part="-1" rank="8"/>
					<player leg="1"  part="0"  rank="1"/>
					<player leg="1"  part="0"  rank="2"/>
					<player leg="1"  part="0"  rank="3"/>
				</players>
			</part>
		</leg>
	</cup>		
		
</tournament>
