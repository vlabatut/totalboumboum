<?xml version="1.0" encoding="utf-8"?>
<theme 	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
		xsi:noNamespaceSchemaLocation="../../../../schemas/theme.xsd" >
	<floors folder="floor">
		<floor name="floor" />
	</floors>
	<blocks>
		<block folder="softwall" name="softwall" />
		<group name="hardwalls" folder="hardwalls">
			<block name="regular" folder="regular"/>
			<block name="inDown" folder="inDown"/>
			<block name="inDownleft" folder="inDownleft"/>
			<block name="inDownright" folder="inDownright"/>
			<block name="inLeft" folder="inLeft"/>
			<block name="inRight" folder="inRight"/>
			<block name="inUp" folder="inUp"/>
			<block name="inUpleft" folder="inUpleft"/>
			<block name="inUpright" folder="inUpright"/>
			<block name="outLeftA" folder="outLeftA"/>
			<block name="outLeftB" folder="outLeftB"/>
			<block name="outRightA" folder="outRightA"/>
			<block name="outRightB" folder="outRightB"/>
		</group>
	</blocks>
	
</theme>
