			<block name="pipebigupleftB" folder="pipebigupleftB"/>
			<block name="pipebiguprightA" folder="pipebiguprightA"/>
			<block name="pipebiguprightB" folder="pipebiguprightB"/>
			<block name="pipesmall" folder="pipesmall"/>
