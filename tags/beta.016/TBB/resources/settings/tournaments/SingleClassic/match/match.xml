<?xml version="1.0" encoding="UTF-8"?>
<match
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:noNamespaceSchemaLocation="../../../../schemas/match.xsd">
	
 	<notes>
 		<line value="Classic match on SBM1 levels. The first player to win 5 round gets the match" />
 		<line value="Author: Vincent Labatut" />
 		<line value="Date: 25-09-2008" />
 	</notes>
 	
	<author value="Vincent Labatut" />

	<limits>
		<points >
			<points local="true" name="total" />
			<threshold value="5" />
			<source local="true" name="total" />
		</points>
	</limits>

 	<rounds randomOrder="true" >
  		<round local="true" name="round1" />
  		<round local="true" name="round2" />
  		<round local="true" name="round3" />
  		<round local="true" name="round4" />
  		<round local="true" name="round5" />
  		<round local="true" name="round6" />
  		<round local="true" name="round7" />
  		<round local="true" name="round8" />
  		<round local="true" name="round9" />
  	</rounds>
	
</match>
