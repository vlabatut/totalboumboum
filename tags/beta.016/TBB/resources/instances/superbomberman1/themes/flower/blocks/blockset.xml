<?xml version="1.0" encoding="utf-8"?>
<blockset 	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
		xsi:noNamespaceSchemaLocation="../../../../../schemas/blockset.xsd" >
	
	<abstractBlocks >
		<block name="common" folder="_common" />
		
		<group name="softwalls" folder="softwalls">
			<block name="common_soft" folder="_common"/>
		</group>

		<group name="hardwalls" folder="hardwalls">
			<block name="common_hard" folder="_common"/>
		</group>
	</abstractBlocks>
	
	<concreteBlocks>
		<group name="softwalls" folder="softwalls">
			<block name="regular" folder="regular"/>
			<block name="spawn" folder="spawn"/>
		</group>
		
		<group name="hardwalls" folder="hardwalls">
			<block name="regular" folder="regular"/>
			
			<block name="insidedownA" folder="insidedownA"/>
			<block name="insidedownB" folder="insidedownB"/>
			<block name="insidedownleft" folder="insidedownleft"/>
			<block name="insidedownright" folder="insidedownright"/>
			<block name="insideleftA" folder="insideleftA"/>
			<block name="insideleftB" folder="insideleftB"/>
			<block name="insiderightA" folder="insiderightA"/>
			<block name="insiderightB" folder="insiderightB"/>
			<block name="insideup" folder="insideup"/>
			<block name="insideupleft" folder="insideupleft"/>
			<block name="insideupright" folder="insideupright"/>
			<block name="outsideleftA" folder="outsideleftA"/>
			<block name="outsideleftB" folder="outsideleftB"/>
			<block name="outsiderightA" folder="outsiderightA"/>
			<block name="outsiderightB" folder="outsiderightB"/>
			<block name="outsideupleft" folder="outsideupleft"/>
			<block name="outsideupright" folder="outsideupright"/>
		</group>
	</concreteBlocks>
	
</blockset>
