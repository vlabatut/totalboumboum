<?xml version="1.0" encoding="utf-8"?>
<blockset 	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
		xsi:noNamespaceSchemaLocation="../../../../../schemas/blockset.xsd" >
	
	<abstractBlocks >
		<block name="common_regular" folder="_common_regular" />
		
		<group name="hardwalls" folder="hardwalls">
			<block name="common_hard" folder="_common"/>
		</group>
		
		<group name="softwalls" folder="softwalls">
			<block name="common_soft" folder="_common"/>
		</group>
	</abstractBlocks>
	
	<concreteBlocks>
		<group name="softwalls" folder="softwalls">
			<block name="regular" folder="regular"/>
		</group>
		
		<group name="hardwalls" folder="hardwalls">
			<block name="regular" folder="regular"/>
		</group>
	</concreteBlocks>
	
</blockset>
