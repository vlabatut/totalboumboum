			<block name="leftup" folder="leftup"/>
			<block name="outsideleftA" folder="outsideleftA"/>
			<block name="outsideleftB" folder="outsideleftB"/>
			<block name="outsiderightA" folder="outsiderightA"/>
			<block name="outsiderightB" folder="outsiderightB"/>
			<block name="rightdown" folder="rightdown"/>
			<block name="rightup" folder="rightup"/>
			<block name="upA" folder="upA"/>
			<block name="upB" folder="upB"/>
			<block name="upleft" folder="upleft"/>
			<block name="upleftcorner" folder="upleftcorner"/>
			<block name="upright" folder="upright"/>
			<block name="uprightcorner" folder="uprightcorner"/>
		</group>
	</concreteBlocks>
	
</blockset>
