<?xml version="1.0" encoding="utf-8"?>
<gesture-modulations xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
		xsi:noNamespaceSchemaLocation="../../../../../schemas/gesture_modulations.xsd" >

	<self-modulations>
	</self-modulations>
	
	<actor-modulations>
<!--  	appear --> 
		<modulation frame="true" strength="1" >
			<action name="appear" direction="all" contact="all"
				tilePosition="all" orientation="all" target="all" />
			<actor-restrictions />
			<target-restrictions />
		</modulation>
	</actor-modulations>
	
	<target-modulations>
<!--  	drop --> 
		<modulation frame="true" strength="1" >
			<action name="drop" direction="all" contact="all"
				tilePosition="all" orientation="all" actor="all" />
			<actor-restrictions />
			<target-restrictions />
		</modulation>
	</target-modulations>
	
	<third-modulations>
	</third-modulations>

</gesture-modulations>
