<?xml version="1.0" encoding="UTF-8"?>
<tournament
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:noNamespaceSchemaLocation="../../../schemas/tournament.xsd">
	
	<general name="Tournament-simple de test"/>

 	<notes>
 		<line value="Here goes a short description of the tournament." />
 		<line value="Author: Vincent Labatut" />
 		<line value="Date: 07-10-2008" />
 	</notes>
 	
	<author value="Vincent Labatut" />

	<cup>
		<options randomizePlayers="true" />
		<legs>
			<leg number="1" >
				<part name="Quart-de-finale 1" number="1">
					<match local="false" name="matchLong" />
					<players>
						<player rank="1"/>
						<player rank="2"/>
						<player rank="3"/>
