					</players>
				</part>
			</leg>

			<leg number="3" >
				<part name="Classement 3" number="1" ranking="4">
					<match local="false" name="matchLong" />
					<players>
						<player part="3" rank="3"/>
						<player part="3" rank="4"/>
						<player part="4" rank="3"/>
						<player part="4" rank="4"/>
					</players>
				</part>
				<part name="Classement 4" number="2" ranking="3">
					<match local="false" name="matchLong" />
					<players>
						<player part="3" rank="1"/>
						<player part="3" rank="2"/>
						<player part="4" rank="1"/>
						<player part="4" rank="2"/>
					</players>
				</part>
				<part name="Petite finale" number="3" ranking="2">
					<match local="false" name="matchLong" />
					<players>
						<player part="1" rank="3"/>
						<player part="1" rank="4"/>
						<player part="2" rank="3"/>
						<player part="2" rank="4"/>
					</players>
				</part>
				<part name="Finale" number="4" ranking="1">
					<match local="false" name="matchLong" />
					<players>
						<player part="1" rank="1"/>
						<player part="1" rank="2"/>
						<player part="2" rank="1"/>
						<player part="2" rank="2"/>
					</players>
				</part>
			</leg>
		</legs>

	</cup>		
		
</tournament>
