					</players>
				</part>
				<part name="Quart-de-finale 2" number="2">
					<match local="false" name="matchLong" />
					<players>
						<player rank="5"/>
						<player rank="6"/>
						<player rank="7"/>
						<player rank="8"/>
					</players>
				</part>
				<part name="Quart-de-finale 3" number="3">
					<match local="false" name="matchLong" />
					<players>
						<player rank="9"/>
						<player rank="10"/>
						<player rank="11"/>
						<player rank="12"/>
					</players>
				</part>
				<part name="Quart-de-finale 4" number="4">
					<match local="false" name="matchLong" />
					<players>
						<player rank="13"/>
						<player rank="14"/>
						<player rank="15"/>
						<player rank="16"/>
					</players>
				</part>
			</leg>
			
			<leg number="2" >
				<part name="Classement 1" number="1">
					<match local="false" name="matchLong" />
					<players>
						<player part="1" rank="3"/>
						<player part="2" rank="4"/>
						<player part="3" rank="3"/>
						<player part="4" rank="4"/>
					</players>
				</part>
				<part name="Classement 2" number="2">
					<match local="false" name="matchLong" />
					<players>
						<player part="1" rank="4"/>
						<player part="2" rank="3"/>
						<player part="3" rank="4"/>
						<player part="4" rank="3"/>
					</players>
				</part>
				<part name="Demi-finale 1" number="3">
					<match local="false" name="matchLong" />
					<players>
						<player part="1" rank="1"/>
						<player part="2" rank="2"/>
						<player part="3" rank="1"/>
						<player part="4" rank="2"/>
					</players>
				</part>
				<part name="Demi-finale 2" number="4">
					<match local="false" name="matchLong" />
					<players>
						<player part="1" rank="2"/>
						<player part="2" rank="1"/>
						<player part="3" rank="2"/>
